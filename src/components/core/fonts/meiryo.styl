@font-face
  font-family: 'Meiryo'
  src: local('Meiryo'),
      url('https://intrnl.github.io/FluentDiscord/assets/fonts/Meiryo.ttf') format('truetype'),
      url('https://cdn.staticaly.com/gh/intrnl/FluentDiscord/master/assets/fonts/Meiryo.ttf') format('truetype')
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Meiryo'
  src: local('Meiryo Italic'), local('Meiryo-Italic'),
      url('https://intrnl.github.io/FluentDiscord/assets/fonts/Meiryo-Italic.ttf') format('truetype'),
      url('https://cdn.staticaly.com/gh/intrnl/FluentDiscord/master/assets/fonts/Meiryo-Italic.ttf') format('truetype')
  font-weight: normal
  font-style: italic

@font-face
  font-family: 'Meiryo'
  src: local('Meiryo Bold'), local('Meiryo-Bold'),
      url('https://intrnl.github.io/FluentDiscord/assets/fonts/Meiryo-Bold.ttf') format('truetype'),
      url('https://cdn.staticaly.com/gh/intrnl/FluentDiscord/master/assets/fonts/Meiryo-Bold.ttf') format('truetype')
  font-weight: bold
  font-style: normal

@font-face
  font-family: 'Meiryo'
  src: local('Meiryo Bold Italic'), local('Meiryo-BoldItalic'),
      url('https://intrnl.github.io/FluentDiscord/assets/fonts/Meiryo-BoldItalic.ttf') format('truetype'),
      url('https://cdn.staticaly.com/gh/intrnl/FluentDiscord/master/assets/fonts/Meiryo-BoldItalic.ttf') format('truetype')
  font-weight: bold
  font-style: italic
